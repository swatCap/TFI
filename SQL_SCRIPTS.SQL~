CREATE USER tfi PASSWORD 'tfisecret'

CREATE USER admin PASSWORD 'swat'

ALTER USER admin ADMIN TRUE

CREATE TABLE FAVOURITES(FAVOURITE_ID BIGINT PRIMARY KEY, SCREEN_NAME VARCHAR(40) NOT NULL, NAME VARCHAR(40) NOT NULL);

CREATE TABLE TRANSACTION_DATES (TRANSACTION_ID INT, DATE TIMESTAMP DEFAULT CURRENT_TIMESTAMP);

CREATE TABLE HISTORY (TRANSACTION_ID INT, TWITTERIAN_ID BIGINT, FOLLOWER BOOLEAN, FRIEND BOOLEAN);
